{
  "workbench.statusBar.feedback.visible": false,
  "breadcrumbs.enabled": true,
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.minimap.showSlider": "always",
  "swaggerViewer.defaultPort": 9867,
  "gitlens.defaultDateFormat": "YYYY M D ddd HH:ss",
  "gitlens.statusBar.alignment": "left",
  "zenMode.hideLineNumbers": false,
  "window.newWindowDimensions": "maximized",
  "better-comments.highlightPlainText": true,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "fixme",
      "color": "#FF0000",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "todo",
      "color": "#FFCC00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    }
  ],
  "workbench.startupEditor": "welcomePage",
  "[yaml]": {
    "editor.tabSize": 2
  },
  "editor.renderLineHighlight": "gutter",
  "C_Cpp.clang_format_path": "/usr/bin/clang-format",
  "black-formatter.interpreter": [],
  // "python.
  "markdown.extension.print.theme": "dark",
  "color-highlight.markRuler": false,
  "color-highlight.markerType": "outline",
  "csharp.referencesCodeLens.enabled": false,
  "workbench.colorTheme": "Monokai",
  "gitlens.codeLens.enabled": false,
  "gitlens.hovers.annotations.changes": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "debug.toolBarLocation": "docked",
  "debug.terminal.clearBeforeReusing": true,
  "redhat.telemetry.enabled": false,
  "telemetry.telemetryLevel": "off",
  "search.useIgnoreFiles": false,
  "editor.largeFileOptimizations": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "vs-kubernetes": {
    "vscode-kubernetes.minikube-path-linux": "/home/sinclair43/.vs-kubernetes/tools/minikube/linux-amd64/minikube"
  },
  "testing.automaticallyOpenPeekView": "never",
  "editor.formatOnSave": true,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
  },
  "python.formatting.provider": "none",
  "window.menuBarVisibility": "toggle",
  "window.zoomLevel": 1,
  "editor.defaultFormatter": null,
  "cmake.configureOnOpen": false,
  "python.autoComplete.extraPaths": [
    "./src"
  ],
  "python.analysis.extraPaths": [
    "./src",
    "./app",
    "./backend",
    "chatgpt"
  ],
  "python.analysis.diagnosticSeverityOverrides": {},
  "python.envFile": ".env",
  "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}${profileName}${separator}",
  "Prettier-SQL.SQLFlavourOverride": "postgresql",
  "Prettier-SQL.keywordCase": "upper",
  "Prettier-SQL.ignoreTabSettings": true,
  "zig.zls.enabled": "off",
  "extensions.ignoreRecommendations": true,
  "python.analysis.typeCheckingMode": "standard",
  "docker.extension.enableComposeLanguageServer": false,
  "vscode-pets.petSize": "small",
  "vscode-pets.theme": "castle",
  "chat.implicitContext.suggestedContext": false,
  "remote.SSH.experimental.chat": false,
  "chat.agent.enabled": false,
  "chat.agent.codeBlockProgress": false,
  "chat.agent.thinking.generateTitles": false,
  "chat.checkpoints.enabled": false,
  "chat.detectParticipant.enabled": false,
  "python.analysis.includeAliasesFromUserFiles": true,
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "git.blame.editorDecoration.enabled": true,
}